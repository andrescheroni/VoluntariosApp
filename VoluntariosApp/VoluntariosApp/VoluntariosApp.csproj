    <None Remove="Resources\Fonts\FASolid.otf" />
    <None Remove="Resources\Fonts\Montserrat-Italic.ttf" />
    <None Remove="Resources\Fonts\Montserrat-Light.ttf" />
    <None Remove="Resources\Fonts\Montserrat-LightItalic.ttf" />
    <None Remove="Resources\Fonts\Montserrat-Regular.ttf" />
    <None Remove="Resources\Images\aceptada.png" />
    <None Remove="Resources\Images\buscar.png" />
    <None Remove="Resources\Images\noimage.png" />
    <None Remove="Resources\Images\pendiente.png" />
    <None Remove="Resources\Images\rechazada.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Fonts\FABrands.otf" />
